{
    "$schema": "http://json.schemastore.org/template",
    "author": "Kiattisak Phanphu",
    "classifications": ["Codezonelive", "Mvc", "ClassLib", "xUnit"],
    "identity": "Codezonelive.Templates.Web",
    "name": "Codezonelive xMvc",
    "shortName": "xmvc",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Codezonelive.Templates",
    "preferNameDirectory": true,
    "symbols": {
        "Framework": {
            "type": "parameter",
            "description": "The target framework for the project.",
            "datatype": "choice",
            "choices": [
              {
                "choice": "net5.0",
                "description": "Target .NET 5.0"
              }
            ],
            "defaultValue": "net5.0"
        }
    },
    "sources": [
        {
            "rename": {
                "_gitignore": ".gitignore"
            }
        }
    ]
}